<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firebasecloudtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseCloudTest">
        <activity android:name=".WindowsActivity"></activity>
        <activity android:name=".AppleActivity" />
        <activity android:name=".SamsungActivity" />
        <activity android:name=".MsiLaptop" />
        <activity android:name=".DellLaptop" />
        <activity android:name=".HpLaptop" />
        <activity android:name=".PizzaActivity" />
        <activity android:name=".FriesActivity" />
        <activity android:name=".HamburgersActivity" />
        <activity android:name=".SmartphonesActivity" />
        <activity android:name=".LaptopsActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>